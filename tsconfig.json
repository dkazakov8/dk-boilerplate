{
  "include": [
    "env.ts",
    "paths.ts",
    "styleguide",
    "src",
    "_webpack"
  ],
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "jsx": "react-jsx",
    "outDir": "build",
    "target": "es2019",
    "module": "esnext",
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "baseUrl": ".",
    "checkJs": false,
    "sourceMap": false,
    "skipLibCheck": true,
    "declaration": false,
    "declarationMap": false,
    "isolatedModules": false,
    "esModuleInterop": true,
    "keyofStringsOnly": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": true,
    "allowSyntheticDefaultImports": true,
    "types": ["google.maps"],
    "paths": {
      "*": [
        "*",
        "src/*",
        "server/*"
      ]
    }
  }
}